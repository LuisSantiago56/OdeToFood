@model IEnumerable<OdeToFood.Data.Models.Restaurant>
@{
    ViewBag.Title = "Index";
}

    <div class="jumbotron">
        <h1>RESTAURANTS</h1>
        <div>There are @Model.Count() restaurants in the db.</div>
    </div>
<ul>
    @foreach (var restaurant in Model)
    {
        <li>@restaurant.Name</li>
    }
</ul>

